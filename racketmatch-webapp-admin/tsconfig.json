{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["DOM", "ESNext"],
    "jsx": "react-jsx",
    "baseUrl": "./src",
"paths": {
  "@pages/*": ["pages/*"],
  "@components/*": ["components/*"],
  "@services/*": ["services/*"],
  "@models/*": ["models/*"],
  "@routes/*": ["routes/*"],
  "@context/*": ["context/*"]
},
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
